<%= simple_form_for(current_user, url: settings_users_change_email_path, html: { method: :put }) do |f| %>
  <div class="form-group">
    <%= f.input :email, autofocus: true, input_html: { class: 'form-control', id: 'email' } %>
  </div>
  <%= f.button :submit, t('update'), class: 'btn_1 rounded' %>
<% end %>